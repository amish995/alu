module boolean (
    input a[8],
    input b[8],
    input alufn[6],
    output bool[8]

  ) {
 var i;

  always {
  bool = 0;

    for (i=0; i<8; i++) {

      case(alufn[3:0]) {

      4b1000: bool[i] = a[i] & b[i]; // AND Case

      4b1110: bool[i] = a[i] | b[i]; // OR Case

      4b0110: bool[i] = a[i] ^ b[i]; // XOR Case

      4b1010: bool[i] = a[i]; // Pass A[i] to Output

      }
    }
  }
}